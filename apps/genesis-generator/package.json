{
  "name": "@axionax/genesis-generator",
  "version": "1.9.0",
  "private": true,
  "description": "Genesis Block Generator for Axionax Mainnet",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "genesis-generator": "./dist/cli.js"
  },
  "scripts": {
    "dev": "tsx watch src/cli.ts",
    "build": "tsup src/cli.ts src/index.ts --format esm --dts",
    "start": "node dist/cli.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules"
  },
  "dependencies": {
    "@axionax/blockchain-utils": "workspace:*",
    "chalk": "^5.6.2",
    "commander": "^12.1.0",
    "inquirer": "^12.11.1",
    "ora": "^8.2.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.9",
    "@types/node": "^22.19.8",
    "eslint": "^9.39.2",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^2.1.9"
  }
}
