{
  "name": "@axionax/genesis-generator",
  "version": "1.9.0",
  "private": true,
  "description": "Genesis Block Generator for Axionax Mainnet",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "genesis-generator": "./dist/cli.js"
  },
  "scripts": {
    "dev": "tsx watch src/cli.ts",
    "build": "tsup src/cli.ts src/index.ts --format esm --dts",
    "start": "node dist/cli.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules"
  },
  "dependencies": {
    "@axionax/blockchain-utils": "workspace:*",
    "commander": "^12.1.0",
    "chalk": "^5.3.0",
    "ora": "^8.1.1",
    "inquirer": "^12.1.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^22.10.2",
    "eslint": "^8.57.0",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
