{
    "name": "@axionax/sdk",
    "version": "1.9.0",
    "private": true,
    "description": "TypeScript SDK for Axionax Protocol",
    "license": "MIT",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        }
    },
    "scripts": {
        "build": "tsup src/index.ts --format esm --dts",
        "dev": "tsup src/index.ts --format esm --dts --watch",
        "test": "vitest run",
        "test:watch": "vitest",
        "lint": "eslint src --ext .ts",
        "type-check": "tsc --noEmit",
        "clean": "rm -rf dist node_modules"
    },
    "dependencies": {
        "@axionax/blockchain-utils": "workspace:*",
        "ethers": "^6.16.0"
    },
    "devDependencies": {
        "@types/node": "^22.19.8",
        "eslint": "^9.39.2",
        "tsup": "^8.5.1",
        "typescript": "^5.9.3",
        "vitest": "^2.1.9"
    }
}